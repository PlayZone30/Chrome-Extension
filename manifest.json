{
  "manifest_version": 3,
  "name": "VisionFlare",
  "version": "1.0",
  "description": "Override the new tab with motivational images",
  "permissions": ["activeTab", "scripting","tabs"],
  "icons": {
    "128": "./icons/128.png",
    "48": "./icons/48.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
